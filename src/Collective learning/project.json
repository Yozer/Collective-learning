    },
    "SFML.Net": "1.0.0-*"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50"
    }
  },

  "runtimes": {
    "win10-x64": {},
    "osx.10.11-x64": {},
    "ubuntu.14.10-x64": {}
  }
